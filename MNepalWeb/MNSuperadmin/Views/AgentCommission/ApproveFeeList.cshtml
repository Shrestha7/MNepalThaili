

@model IEnumerable<MNSuperadmin.Models.UserInfo>




@{
    ViewBag.Title = "Fee Approve List";
}

<section class="content-header">
    <h1>
        Agent Commission
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> @ViewContext.RouteData.Values["Controller"].ToString()</a></li>
        <li class="active">Approve Fee</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">
        <div class="col-md-12">
            <!-- Start Form -->
            <!-- Start Data Show in Table -->
            <div id="FeeApprove" class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Approve Fee</h3>
                </div>
                <!-- /.box-header -->




                @if (this.ViewData["feeapprove_message"] != null)
                {
                    if (this.ViewData["message_class"].ToString() == "success_info")
                    {
                        <div class="alert alert-success">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Success</h4> @this.ViewData["feeapprove_message"]
                        </div>
                    }
                    else
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <h4>Error</h4> @this.ViewData["feeapprove_message"]
                        </div>
                    }
                }

                <div class="box-body">
                        <div class="box-body" id="grid">
                            <table id="PinApprove" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>Fee ID</th>
                                        <th>Fee Type</th>
                                        <th>Details</th>
                                        <th>Percentage</th>
                                        <th>Flat Free</th>
                                        <th>Action</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model.Count() > 0)
                                    {
                                        foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>@item.FeeID</td>
                                                <td>@item.FeeType</td>
                                                <td>@item.Details</td>
                                                <td>@item.Percentage</td>
                                                <td>@item.FlatFee</td>
                                                <td>
                                                    <a href="@Url.Action("ViewApproveFee","AgentCommission", new {FeeID = @item.FeeID } )"><span class="fa fa-eye" title="View"></span></a> 
                                                </td>


                                            </tr>
                                        }
                                    }
                                    else
                                    {
                                        <tr>
                                            <td colspan="7" style="text-align:center">NO RECORDS FOUND!</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    
                </div>
                <!-- /.box-body -->

            </div>

        </div>
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->



</MNSuperadmin.Models.UserInfo>